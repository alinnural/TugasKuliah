@model IEnumerable<IvanHotel.ViewModels.TransaksiVM>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Back/_Layout.cshtml";
}
<div class="box">
    <div class="box-header">
        <b>Editor Tipe Kontak</b>
    </div>
    <div class="box-body">
        <table class="table table-responsive table-striped" id="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Nama)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NomorIdentitas)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.JenisKelamin)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TMT)
                    </th>
                    <th>
                        Registrasi
                    </th>
                    <th></th>
                </tr>
            </thead>
           <tbody>
               @foreach (var item in Model)
               {
                   <tr>
                       <td>
                           @Html.DisplayFor(modelItem => item.Nama)
                       </td>
                       <td>
                           @Html.DisplayFor(modelItem => item.NomorIdentitas)
                       </td>
                       <td>
                           @Html.DisplayFor(modelItem => item.JenisKelamin)
                       </td>
                       <td>
                           @Html.DisplayFor(modelItem => item.TMT)
                       </td>
                       <td>
                           @Html.DisplayFor(modelItem => item.Registrasi)
                       </td>
                       <td width="200">
                           <div class="btn-group">
                               @Html.ActionLink("Verifikasi", "Verifikasi", new { id = item.ID }, new { @class = "btn btn-warning btn-sm btn-flat" })
                               @Html.ActionLink("Details", "Details", new { id = item.ID }, new { @class = "btn btn-primary btn-sm btn-flat" })
                               @*@Html.ActionLink("Delete", "Delete", new { id = item.ID }, new { @class = "btn btn-danger btn-sm btn-flat" })*@
                           </div>
                       </td>
                   </tr>
               }
           </tbody>
        </table>
    </div>
</div>
@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#table').DataTable();
        });
    </script>
}
